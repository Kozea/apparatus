{
  "name": "apparatus",
  "version": "1.0.0",
  "description": "Test of react 4th edition",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/lib/frontend/src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|sass)$": "<rootDir>/lib/tests/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/lib/tests/setup.js"
    ]
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "private": true,
  "scripts": {
    "serve": "NODE_ENV=production node dist/server.js",
    "serve-server": "WEBPACK_ENV=server NODE_ENV=development webpack",
    "serve-client": "WEBPACK_ENV=browser NODE_ENV=development webpack-dev-server",
    "build-client": "WEBPACK_ENV=browser NODE_ENV=production webpack",
    "build-server": "WEBPACK_ENV=server NODE_ENV=production webpack",
    "lint": "eslint --cache --ext .jsx --ext .js lib/",
    "fix": "prettier --write '{,lib/tests/**/,lib/frontend/src/**/}*.js?(x)'",
    "test": "node ./node_modules/jest-cli/bin/jest.js --no-cache",
    "test-debug": "inspect ./node_modules/jest-cli/bin/jest.js --runInBand --env jest-environment-node-debug",
    "lint-all": "make fix lint"
  },
  "pre-commit": "lint-all",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bemboo": "^1.0.0",
    "date-fns": "2.0.0-alpha.7",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "koaze": "0.1.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "redbox-react": "1.5.0",
    "redux": "3.7.2",
    "redux-api-unrest": "^0.6.1",
    "redux-http-status": "^0.0.10",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.38",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/register": "^7.0.0-beta.38",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "css-loader": "0.28.9",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.16.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.5.1",
    "file-loader": "1.1.6",
    "ignore-loader": "0.1.2",
    "import-sort-style-module": "4.2.0",
    "jest": "22.1.4",
    "jest-environment-node-debug": "2.0.0",
    "json-loader": "0.5.7",
    "node-sass": "4.7.2",
    "pre-commit": "1.2.2",
    "prettier": "1.10.2",
    "react-test-renderer": "16.2.0",
    "resolve-url-loader": "2.2.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "webpackozea": "0.0.12"
  }
}
